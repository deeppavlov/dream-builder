{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "plugin:react/jsx-runtime",
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "node": true
  },
  "plugins": [
    "react",
    "@typescript-eslint",
    "no-relative-import-paths"
    // "unused-imports"
  ],
  "rules": {
    "no-relative-import-paths/no-relative-import-paths": [
      "warn",
      { "allowSameFolder": true, "rootDir": "src" }
    ]
  }
}
